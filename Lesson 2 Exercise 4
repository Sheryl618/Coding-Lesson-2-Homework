score = input('Your score:')
score_number = int(score)
if score_number >= 90: print('You got a A!')
elif 80 <= score_number < 90: print('You got a B')
elif 70 <= score_number < 80: print('You got a C')
elif 60 <= score_number < 70: print('You got a D')
elif score_number < 60: print('You got a F. Good luck next time.')
