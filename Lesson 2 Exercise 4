score = input('Your score:')
score_number = int(score)
if score_number >= 90: print('You got a A!')
if 80 <= score_number < 90: print('You got a B')
if 70 <= score_number < 80: print('You got a C')
if 60 <= score_number < 70: print('You got a D')
if score_number < 60: print('You got a F. Good luck next time.')
